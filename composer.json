{
    "name": "mention/fast-doctrine-paginator",
    "description": "Fast Doctrine paginator for batching, infinite scrolling, Relay",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Le Blanc",
            "email": "arnaud.lb@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mention\\FastDoctrinePaginator\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": ["tests"],
        "psr-4": {
            "Mention\\FastDoctrinePaginator\\Tests\\": "tests"
        }
    },
    "require": {
        "beberlei/assert": "^3.2",
        "mention/kebab": "^1.1.0",
        "mention/paginator": "^1.0.0"
    },
    "require-dev": {
        "doctrine/orm": ">=2.5.0",
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "mockery/mockery": "^1.3",
        "phing/phing": "^2.16",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-beberlei-assert": "^0.12.0",
        "phpstan/phpstan-mockery": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpunit/phpunit": "^8.5.0",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
